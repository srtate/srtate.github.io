---
import PubTag from "../components/PubTag.astro";

interface Props {
    data: {
    authors: string;
    title: string;
    pubin: string;
    extra: string;
    tags: string[];
  };
  url: string;
}

const { data, url } = Astro.props
---
{data.authors}. <a href={url}>{data.title}</a>, in <i>{data.pubin}</i>, {data.extra}. 
  { data.tags.map((t: string) => (<PubTag tag={t} /> ))}
